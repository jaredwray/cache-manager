{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "declaration": true,
    "incremental": true,
    "downlevelIteration": true
  },
  "exclude": ["dist"]
}
